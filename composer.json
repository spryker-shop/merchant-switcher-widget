{
  "name": "spryker-shop/merchant-switcher-widget",
  "description": "MerchantSwitcherWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker-shop/shop-application": "^1.0.0",
    "spryker/kernel": "^3.33.0",
    "spryker/merchant-search": "^0.1.0",
    "spryker/symfony": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {},
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "spryker/router": "*"
  },
  "suggest": {
    "spryker/router": "Use this module when you want to use the Router."
  }
}
